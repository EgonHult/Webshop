@model List<Product>
@{
    ViewData["Title"] = "Index";
}

<h1>Products</h1>
<table class="table">
    <thead>
        <tr>
            <th>ProductName</th>
            <th>Price</th>
            <th>Quantity</th>
            <th>Description</th>
        </tr>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.Name </td>
                <td>@product.Price</td>
                <td>@product.Quantity</td>
                <td>@product.Description</td>
            </tr>



        }

    </thead>
</table>

